{
  "name": "blogs-api",
  "version": "1.0.0",
  "description": "Este projeto é uma API que se conecta a um banco de dados para a produção de conteúdo para um blog",
  "main": "src/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prestart": "npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "create": "npx sequelize-cli db:create",
    "drop": "npx sequelize-cli db:drop",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "start": "nodemon .",
    "lint": "eslint --no-inline-config --no-error-on-unmatched-pattern -c .eslintrc.json .",
    "lint-fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brenndha-cabral/blogs-api.git"
  },
  "keywords": [],
  "author": "Brenndha Cabral",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/brenndha-cabral/blogs-api/issues"
  },
  "homepage": "https://github.com/brenndha-cabral/blogs-api#readme",
  "dependencies": {
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.3",
    "sequelize": "^6.21.0",
    "sequelize-cli": "^6.4.1"
  },
  "devDependencies": {
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "nodemon": "^2.0.18"
  }
}
